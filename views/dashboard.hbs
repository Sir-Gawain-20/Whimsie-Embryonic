<h1>dashboard</h1>
<h3>Welcome {{firstName}}</h3>

{{#if proposals }}
    <table class="striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Date</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
            {{#each proposals}}
                <tr>
                    <td><a href="/proposals/{{_id}}">{{title}}</a></td>
                    <td>{{createAt}}</td>
                    <td><span class="dash-status">{{status}}</span></td>
                    <td></td>
                </tr>
            {{/each}}
        </tbody>
    </table>
{{else}}
<p>You have no proposals</p>
{{/if}}
